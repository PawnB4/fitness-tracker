{
	"$schema": "https://biomejs.dev/schemas/2.2.0/schema.json",
	"assist": {
		"actions": {
			"source": {
				"organizeImports": "on",
				"useSortedAttributes": "on",
				"useSortedProperties": "on"
			}
		},
		"enabled": true
	},
	"files": {
		"ignoreUnknown": false
	},
	"formatter": {
		"enabled": true,
		"indentStyle": "tab",
		"lineEnding": "crlf"
	},
	"javascript": {
		"formatter": {
			"quoteStyle": "double"
		}
	},
	"linter": {
		"enabled": true,
		"rules": {
			"complexity": {
				"noForEach": "off",
				"noUselessFragments": {
					"fix": "safe",
					"level": "error"
				},
				"useLiteralKeys": "off"
			},
			"correctness": {
				"noUnusedFunctionParameters": {
					"fix": "safe"
				},
				"noUnusedImports": {
					"fix": "safe"
				},
				"noUnusedVariables": "off",
				"useParseIntRadix": "off"
			},
			"nursery": {
				"useSortedClasses": {
					"fix": "safe",
					"level": "error"
				}
			},
			"recommended": true,
			"style": {
				"useSelfClosingElements": "error",
				"useTemplate": {
					"fix": "safe",
					"level": "error"
				}
			},
			"suspicious": {
				"noGlobalIsNan": "off",
				"useIterableCallbackReturn": "off",
				"noTsIgnore": "off"
			},
			"a11y": {
				"useSemanticElements": "off"
			}
		}
	},
	"vcs": {
		"clientKind": "git",
		"enabled": false,
		"useIgnoreFile": false
	}
}